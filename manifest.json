{
  "manifest_version": 3,
  "name": "Link Saver",
  "version": "1.3",
  "description": "Save and load your favorite links",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}